    <!-- 保留profiles，但只包含变量定义 -->
    <profiles>
        <profile>
            <id>local</id>
            <properties>
                <!-- 环境标识，需要与配置文件的名称相对应 -->
                <profiles.active>local</profiles.active>
                <logging.level>debug</logging.level>
            </properties>
        </profile>
        <profile>
            <id>dev</id>
            <properties>
                <!-- 环境标识，需要与配置文件的名称相对应 -->
                <profiles.active>dev</profiles.active>
                <logging.level>debug</logging.level>
            </properties>
            <activation>
                <!-- 默认环境 -->
                <activeByDefault>true</activeByDefault>
            </activation>
        </profile>
        <profile>
            <id>test</id>
            <properties>
                <profiles.active>test</profiles.active>
                <logging.level>debug</logging.level>
                <deploy.snapshot.id>test-snapshots</deploy.snapshot.id>
                <deploy.release.id>test-releases</deploy.release.id>
            </properties>
        </profile>
        <profile>
            <id>prod</id>
            <properties>
                <profiles.active>prod</profiles.active>
                <logging.level>debug</logging.level>
                <deploy.snapshot.id>prod-snapshots</deploy.snapshot.id>
                <deploy.release.id>prod-releases</deploy.release.id>
            </properties>
        </profile>
    </profiles>
